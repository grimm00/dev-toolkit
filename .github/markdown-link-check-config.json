{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/yourusername/"
    },
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^\\.\\./.*\\.md$"
    },
    {
      "pattern": "^[^/]*\\.md$"
    },
    {
      "pattern": "^[^/]*/[^/]*\\.md$"
    },
    {
      "pattern": "^[^/]*/[^/]*/[^/]*\\.md$"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206],
  "ignoreFiles": [
    "admin/planning/releases/**/*.md",
    "admin/planning/notes/**/*.md",
    "admin/planning/features/**/*.md",
    "admin/docs/guides/**/*.md",
    "admin/planning/roadmap.md"
  ]
}
